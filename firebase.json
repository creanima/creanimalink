{
  "hosting": {
    "site": "mofti-link",
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "dynamicLinks": true
      },
      {
        "source": "assets/**",
        "dynamicLinks": false
      }
    ]
  }
}
